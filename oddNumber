public class oddNumbers {
	
	public static void main (String args []) {
		//TODO Auto-genaratrd method stub
		
		int sum=0;
		
		    for (int i=7; i<=21; i=i+2) {
		    	if(i % 2!=0){
		    		sum +=i;
		    	}
		    	System.out.println(i);
		    }
		 System.out.println("the sum of odd numbers form 7to 21:"+ sum);
		    }
}
